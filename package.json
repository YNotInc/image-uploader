{
  "name": "rest-shop",
  "version": "1.0.0",
  "description": "A nod.js RESTful API Tutorial Project",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "start:server": "node ./server.js",
    "start:client": "cd client && npm start",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "test": "echo \"Error: no test specified\"",
    "test:mocha": "mocha 'test/**/*.spec.js'",
    "test:jest": "cd client && npm run test:jest",
    "test:postman": "concurrently \"node server.js\" \"cd postmanTests && newman run Image-Uploader.postman_collection.json --delay-request 50 -r cli, json -e Localhost.postman_environment.json\"",
    "coverage": "nyc npm run test:mocha"
  },
  "author": "Tony Lockhart",
  "license": "ISC",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.21.0",
    "concurrently": "^4.1.2",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-form-data": "^2.0.13",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "sequelize": "^5.21.12",
    "typescript": "^4.0.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.0.1",
    "morgan": "^1.9.1",
    "newman": "^5.2.4",
    "nodemon": "^1.18.7",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "react-scripts": "^4.0.1",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0"
  }
}